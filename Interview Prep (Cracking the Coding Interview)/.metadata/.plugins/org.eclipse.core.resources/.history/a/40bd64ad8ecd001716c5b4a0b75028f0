
public class Problem1_4 {
	
	
	public String addUnicode(String word){
		char[] result = word.toCharArray();
		for(int i = 0; i < result.length; i++){
			if(result[i] == ' '){
				result[i] = '%';
				for(int j = result.length; j > i+1; j--){
					result[j+2] = result[j+1];
					result[j+1] = result[j];
				}
			}
		}
		return new String(result);
	}
}
